{
  "name": "@commencis/eslint-plugin",
  "version": "1.0.0",
  "description": "Commencis ESLint plugin",
  "author": "Commencis WEB Team",
  "license": "Apache-2.0",
  "type": "module",
  "homepage": "https://github.com/Commencis/js-toolkit/packages/eslint-plugin#readme",
  "bugs": {
    "url": "https://github.com/Commencis/js-toolkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Commencis/js-toolkit.git",
    "directory": "packages/eslint-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "*.md",
    "LICENSE"
  ],
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/utils": "8.0.0-alpha.30"
  },
  "devDependencies": {
    "@commencis/ts-config": "workspace:*",
    "tsup": "8.1.0",
    "typescript": "5.4.5"
  }
}
