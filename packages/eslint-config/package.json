{
  "name": "@commencis/eslint-config",
  "version": "2.5.0",
  "description": "Commencis ESLint config",
  "author": "Commencis WEB Team",
  "license": "Apache-2.0",
  "type": "module",
  "homepage": "https://github.com/Commencis/js-toolkit/tree/main/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/Commencis/js-toolkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Commencis/js-toolkit.git",
    "directory": "packages/eslint-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "*.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./javascript": {
      "types": "./dist/configs/javascript.d.ts",
      "import": "./dist/configs/javascript.js"
    },
    "./typescript": {
      "types": "./dist/configs/typescript.d.ts",
      "import": "./dist/configs/typescript.js"
    },
    "./react": {
      "types": "./dist/configs/react.d.ts",
      "import": "./dist/configs/react.js"
    },
    "./next": {
      "types": "./dist/configs/next.d.ts",
      "import": "./dist/configs/next.js"
    },
    "./native": {
      "types": "./dist/configs/native.d.ts",
      "import": "./dist/configs/native.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "16.1.1",
    "@typescript-eslint/utils": "8.51.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "17.0.0",
    "typescript-eslint": "8.51.0"
  },
  "devDependencies": {
    "@commencis/ts-config": "1.0.0",
    "eslint": "9.39.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eslint": ">=9.39"
  }
}
