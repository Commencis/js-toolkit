  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@stylistic/stylelint-config": "3.0.1",
    "postcss-styled-syntax": "0.7.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier-scss": "1.0.0",
